{
    "name": "whitecube/laravel-preset",
    "description": "Custom Laravel Preset for the Whitecube workflow.",
    "keywords": [
        "whitecube",
        "laravel-preset"
    ],
    "homepage": "https://github.com/whitecube/laravel-preset",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Adrien Leloup",
            "email": "adrien@whitecube.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/ui": "^3.0",
        "whitecube/bem-components": "^1.1",
        "whitecube/laravel-sluggable": "^1.6",
        "barryvdh/laravel-debugbar": "^3.6",
        "pestphp/pest-plugin-laravel": "^1.2.0"
    },
    "require-dev": {
        "jubeki/laravel-code-style": "^1.0",
        "laravel/helpers": "^1.3",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0",
        "spatie/laravel-log-dumper": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Whitecube\\LaravelPreset\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": ["Whitecube\\LaravelPreset\\WhitecubeServiceProvider"]
        }
    }
}
