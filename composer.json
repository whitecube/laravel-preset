{
    "name": "whitecube/laravel-preset",
    "description": "Custom Laravel Preset for the Whitecube workflow.",
    "keywords": [
        "whitecube",
        "laravel-preset"
    ],
    "homepage": "https://github.com/whitecube/laravel-preset",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Adrien Leloup",
            "email": "adrien@whitecube.be",
            "role": "Developer"
        },
        {
            "name": "Théo Léonet",
            "email": "theo@whitecube.be",
            "role": "Developer"
        },
        {
            "name": "Toon Van den Bos",
            "email": "toon@whitecube.be",
            "role": "Developer"
        },
    ],
    "require": {
        "php": "^8.0",
        "laravel/prompts": "^0.1.13",
        "laravel/ui": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Whitecube\\LaravelPreset\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": ["Whitecube\\LaravelPreset\\WhitecubeServiceProvider"]
        }
    }
}
